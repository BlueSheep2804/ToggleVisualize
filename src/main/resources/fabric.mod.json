{
	"schemaVersion": 1,
	"id": "togglevisualize",
	"version": "${version}",
	"name": "Toggle Visualize",
	"description": "Visualize sprint and sneak toggling.",
	"authors": [
		"BlueSheep2804"
	],
	"contact": {
		"sources": "https://github.com/BlueSheep2804/ToggleVisualize",
		"issues": "https://github.com/BlueSheep2804/ToggleVisualize/issues"
	},
	"license": "MIT",
	"icon": "assets/togglevisualize/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			{
				"value": "io.github.bluesheep2804.togglevisualize.ToggleVisualize",
				"adapter": "kotlin"
			}
		],
		"modmenu": [
			"io.github.bluesheep2804.togglevisualize.ModMenuIntegration"
		]
	},
	"depends": {
		"fabricloader": ">=0.16.9",
		"minecraft": "~${minecraftVersion}",
		"java": ">=21",
		"fabric-api": "*",
		"fabric-language-kotlin": "*",
		"yet_another_config_lib_v3": ">=${yaclVersion}"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/toggle-visualize",
				"modmenu.modrinth": "https://modrinth.com/mod/toggle-visualize",
				"modmenu.kofi": "https://ko-fi.com/bluesheep"
			}
		}
	}
}